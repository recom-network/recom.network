{
  "name": "recom.network",
  "version": "0.0.12",
  "description": "",
  "scripts": {
    "start": "npm run pi && npm run build:client && npm run build:clean && cross-env NODE_ENV=production node dist/app.js",
    "pi": "npm i && cd client && npm i",
    "dev": "concurrently --kill-others \"npm run dev:server\" \"cd client && npm run dev:client\"",
    "dev:server": "cross-env NODE_ENV=development ts-node ./src/app.ts",
    "build:clean": "tsc --build --clean && tsc --build",
    "build:client": "cd client && npm run build"
  },
  "keywords": [
    "ts",
    "postgres"
  ],
  "author": "Andrey Andrushin <neil.kelly1024@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@types/config": "^0.0.38",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "config": "^3.3.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3"
  }
}
